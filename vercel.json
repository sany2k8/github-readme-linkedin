{
  "builds": [
    {
      "src": "./src/images-renderer/routes/index.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/scraper-server/index.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/images-renderer/routes/education.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/images-renderer/routes/experience.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/images-renderer/routes/languages.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/images-renderer/routes/skills.ts",
      "use": "@now/node"
    },
    {
      "src": "./src/images-renderer/routes/user.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/images-renderer/routes/index.ts"
    },
    {
      "src": "/data",
      "dest": "/src/scraper-server/index.ts"
    },
    {
      "src": "/education",
      "dest": "/src/images-renderer/routes/education.ts"
    },
    {
      "src": "/experience",
      "dest": "/src/images-renderer/routes/experience.ts"
    },
    {
      "src": "/languages",
      "dest": "/src/images-renderer/routes/languages.ts"
    },
    {
      "src": "/skills",
      "dest": "/src/images-renderer/routes/skills.ts"
    },
    {
      "src": "/user",
      "dest": "/src/images-renderer/routes/user.ts"
    }
  ]
}
